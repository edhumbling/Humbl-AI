# Humbl AI Environment Variables
# Copy this file to .env.local and fill in your actual values

# Groq API Configuration
# Get your API key from: https://console.groq.com/keys

# Application Configuration
NEXT_PUBLIC_APP_NAME=Humbl AI
NEXT_PUBLIC_APP_DESCRIPTION=World's Most Powerful Search Engine

# Development/Production Environment
NODE_ENV=development

# Database Configuration (Neon PostgreSQL)
# Get your connection string from: https://console.neon.tech
DATABASE_URL=your_neon_database_connection_string_here

# Stack Auth Configuration
# Get your credentials from: https://app.stack-auth.com
NEXT_PUBLIC_STACK_PROJECT_ID=your_project_id_here
NEXT_PUBLIC_STACK_PUBLISHABLE_CLIENT_KEY=your_publishable_key_here
STACK_SECRET_SERVER_KEY=your_secret_key_here

